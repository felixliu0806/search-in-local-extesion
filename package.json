{
  "name": "search-in-local-extension",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:app && npm run build:content",
    "build:app": "vite build --config vite.config.ts",
    "build:content": "vite build --config vite.content.config.ts",
    "build-extension": "npm run build && node create-icons.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.267",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.48",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "playwright": "^1.48.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/dom": "^10.4.0"
  }
}
